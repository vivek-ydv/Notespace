{
    "builds": [
        {
            "src": "./backend/index.js", 
            "use": "@vercel/node"
        },
        {
            "src": "./build", 
            "use": "@vercel/static"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/backend/index.js"
        }
    ]
}