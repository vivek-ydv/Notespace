{
    "builds": [
        {
            "src": "/backend/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "package.json",
            "use": "vercel-sapper"
        }
    ],
    "routes": [
        {
            "src": "/backend/(.*)",
            "dest": "index.js "
        }
    ]
}